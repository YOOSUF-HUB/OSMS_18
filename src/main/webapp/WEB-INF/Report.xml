<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
<servlet>
     	<servlet-name>CreateReportServlet</servlet-name>
     	<servlet-class>ReportPackage.CreateReportServlet</servlet-class>
</servlet>

<servlet-mapping>
     	<servlet-name>CreateReportServlet</servlet-name>
     	<url-pattern>/CreateReportServlet</url-pattern>
</servlet-mapping>

<servlet>
     	<servlet-name>ViewReportsServlet</servlet-name>
     	<servlet-class>ReportPackage.ViewReportsServlet</servlet-class>
</servlet>

<servlet-mapping>
     	<servlet-name>ViewReportsServlet</servlet-name>
     	<url-pattern>/ViewReportsServlet</url-pattern>
</servlet-mapping>
         
         
<servlet>
     	<servlet-name>ViewReportServlet</servlet-name>
     	<servlet-class>ReportPackage.ViewReportServlet</servlet-class>
</servlet>

<servlet-mapping>
     	<servlet-name>ViewReportServlet</servlet-name>
     	<url-pattern>/ViewReportServlet</url-pattern>
</servlet-mapping>         
         
         
         
<servlet>
     	<servlet-name>UpdateReportServlet</servlet-name>
     	<servlet-class>ReportPackage.UpdateReportServlet</servlet-class>
</servlet>

<servlet-mapping>
     	<servlet-name>UpdateReportServlet</servlet-name>
     	<url-pattern>/UpdateReportServlet</url-pattern>
</servlet-mapping>         
         

<servlet>
     	<servlet-name>DeleteReportServlet</servlet-name>
     	<servlet-class>ReportPackage.DeleteReportServlet</servlet-class>
</servlet>

<servlet-mapping>
     	<servlet-name>DeleteReportServlet</servlet-name>
     	<url-pattern>/DeleteReportServlet</url-pattern>
</servlet-mapping>
 

  
         </web-app>