<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


     <!-- Stock Management -->
	
	<!-- Insert Stock -->
     <servlet>
     	<servlet-name>stockInsertServlet</servlet-name>
     	<servlet-class>onlineStockManagement.stockInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>stockInsertServlet</servlet-name>
     	<url-pattern>/stockInsertServlet</url-pattern>
     </servlet-mapping>

	<!-- View Stock -->
     <servlet>
     	<servlet-name>GetAllStockServlet</servlet-name>
     	<servlet-class>onlineStockManagement.GetAllStockServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>GetAllStockServlet</servlet-name>
     	<url-pattern>/GetAllStockServlet</url-pattern>
     </servlet-mapping>
     
     
     <!-- Update Stock -->
      <servlet>
     	<servlet-name>updateStockServlet</servlet-name>
     	<servlet-class>onlineStockManagement.updateStockServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>updateStockServlet</servlet-name>
     	<url-pattern>/updateStockServlet</url-pattern>
     </servlet-mapping>
     
          <!-- Delete Stock -->
      <servlet>
     	<servlet-name>DeleteStockServlet</servlet-name>
     	<servlet-class>onlineStockManagement.DeleteStockServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>DeleteStockServlet</servlet-name>
     	<url-pattern>/DeleteStockServlet</url-pattern>
     </servlet-mapping>
     
     
     
     
     
     
     <!-- Supplier Management -->
     
     	<!-- Insert Supplier -->
     <servlet>
     	<servlet-name>supplierInsertServlet</servlet-name>
     	<servlet-class>SupplierPackage.supplierInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>supplierInsertServlet</servlet-name>
     	<url-pattern>/supplierInsertServlet</url-pattern>
     </servlet-mapping>
     
     
     
     	<!-- View Supplier -->
     <servlet>
     	<servlet-name>GetAllSupplierServlet</servlet-name>
     	<servlet-class>SupplierPackage.GetAllSupplierServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>GetAllSupplierServlet</servlet-name>
     	<url-pattern>/GetAllSupplierServlet</url-pattern>
     </servlet-mapping>
     
     
     
     
          <!-- Update Supplier -->
      <servlet>
     	<servlet-name>updateSupplierServlet</servlet-name>
     	<servlet-class>SupplierPackage.updateSupplierServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>updateSupplierServlet</servlet-name>
     	<url-pattern>/updateSupplierServlet</url-pattern>
     </servlet-mapping>
     
          	<!-- Delete Supplier -->
     <servlet>
     	<servlet-name>DeleteSupplierServlet</servlet-name>
     	<servlet-class>SupplierPackage.DeleteSupplierServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>DeleteSupplierServlet</servlet-name>
     	<url-pattern>/DeleteSupplierServlet</url-pattern>
     </servlet-mapping>
     
     
     
     
     
     
     
     
          <!-- User(STAFF) Management -->
     
               	<!-- User Insert Register -->
     <servlet>
     	<servlet-name>UserInsertServlet</servlet-name>
     	<servlet-class>UserPackage.UserInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UserInsertServlet</servlet-name>
     	<url-pattern>/UserInsertServlet</url-pattern>
     </servlet-mapping>
     
     
                    	<!-- User Insert Login -->
     <servlet>
     	<servlet-name>LoginServlet</servlet-name>
     	<servlet-class>UserPackage.LoginServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>LoginServlet</servlet-name>
     	<url-pattern>/LoginServlet</url-pattern>
     </servlet-mapping>
     
     
     
     
     
     
          <!-- Stock Order Management -->
     
                         	<!-- StockOrder Insert -->
     <servlet>
     	<servlet-name>StockOrderInsertServlet</servlet-name>
     	<servlet-class>StockOrderManagement.StockOrderInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>StockOrderInsertServlet</servlet-name>
     	<url-pattern>/StockOrderInsertServlet</url-pattern>
     </servlet-mapping>


						<!-- View Stock Orders -->

    <servlet>
        <servlet-name>StockOrdersServlet</servlet-name>
        <servlet-class>StockOrderManagement.StockOrdersServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StockOrdersServlet</servlet-name>
        <url-pattern>/StockOrdersServlet</url-pattern>
    </servlet-mapping>
    
						<!-- Update Stock Orders -->

    <servlet>
        <servlet-name>StockOrderUpdateServlet</servlet-name>
        <servlet-class>StockOrderManagement.StockOrderUpdateServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StockOrderUpdateServlet</servlet-name>
        <url-pattern>/StockOrderUpdateServlet</url-pattern>
    </servlet-mapping>
    
    						<!-- Delete Stock Orders -->

    <servlet>
        <servlet-name>DeleteStockOrderServlet</servlet-name>
        <servlet-class>StockOrderManagement.DeleteStockOrderServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteStockOrderServlet</servlet-name>
        <url-pattern>/DeleteStockOrderServlet</url-pattern>
    </servlet-mapping>


</web-app>